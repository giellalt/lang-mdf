+V+Ind+Prt2:%> INDPRT2-SC ;
+V+Conj:%> INDPRT2-SC ;
+V+Cond-Conj:ндяря%> INDPRT2-SC ;
+V+Des:лекс%> INDPRT2-SC ;
+V: # ;
+V:%>О1 Non-Finite-Vws ;
+V: Non-Finite-Cns ;

LEXICON V-njaftems
+V+Ind:%> PRS-SC ;
+V: # ;
+V:%>О1 Non-Finite-Vws ;
+V:%> Non-Finite-Cns ;

LEXICON V-alyjaftoms
+V+Ind:%> PRS-SC ;
+V: # ;
+V:%>О1 Non-Finite-Vws ;
+V:%> Non-Finite-Cns ;

LEXICON V-andovoms
+V+Ind:%> PRS-SC ;
+V: # ;
+V:%>О1 Non-Finite-Vws ;
+V:%> Non-Finite-Cns ;

LEXICON Non-Finite-Vws
