!!!!!!!!!!!!!!!!!!!!!!
!! SUBJECT ONLY VERBS 
!!!!!!!!!!!!!!!!!!!!!!


LEXICON IV_КУНДАМС
! кундамс:кунда
+V+IV: V0_КУНДАМС ;

LEXICON IV_АНДОМС
!! андомс:анд
+V+IV: V0_АНДОМС ;

LEXICON IV_НЯФТЕМС
+V+IV: V0_НЯФТЕМС ;

LEXICON IV_АЛЫЯФТОМС
+V+IV: V0_АЛЫЯФТОМС ;

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!! VERBS MUTUAL FOR TV and IV !!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

LEXICON V0_КУНДАМС
 VERBAL-CONJUGATIONS-ALL ;
:%> Non-Finite-Vws ;
 Non-Finite-Cns ;

LEXICON V0_АНДОМС
 VERBAL-CONJUGATIONS-ALL ;
:%>О1 Non-Finite-Vws ;
 Non-Finite-Cns ;

LEXICON V0_НЯФТЕМС
 VERBAL-CONJUGATIONS-ALL ;
:%>О1 Non-Finite-Vws ;
:%> Non-Finite-Cns ;

LEXICON V0_АЛЫЯФТОМС
 VERBAL-CONJUGATIONS-ALL ;
:%>О1 Non-Finite-Vws ;
:%> Non-Finite-Cns ;

LEXICON Non-Finite-Vws
+Inf:О1мс K ;
+Ger:О1зь K ;

LEXICON Non-Finite-Cns
+PrtPrc:%^Devoice%>ф K ;


